{
  "name": "discord-kicker-bot",
  "version": "1.0.0",
  "description": "Kicker bot",
  "main": "dist/app.js",
  "repository": "https://github.com/observation-point/kicker-bot",
  "author": "deziev",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "bin/environment node dist/app.js | pino-pretty -t -c",
